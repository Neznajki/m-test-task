{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.3",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "neznajki/authorization-client": "^0.1.0",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/asset": "4.3.*",
        "symfony/cache": "4.3.*",
        "symfony/console": "4.3.*",
        "symfony/dotenv": "4.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.3.*",
        "symfony/monolog-bundle": "^3.4",
        "symfony/orm-pack": "^1.0",
        "symfony/security": "4.3.*",
        "symfony/security-bundle": "4.3.*",
        "symfony/twig-pack": "^1.0",
        "symfony/yaml": "4.3.*",
        "symfony/maker-bundle": "^1.14"
    },
    "require-dev": {
        "neznajki/codeception-test-engine": "v0.0.*",
        "symfony/var-dumper": "4.3.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
            "php": "7.3.10"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        },
        "minimum-stability": "stable",
        "require": {}
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
